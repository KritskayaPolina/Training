public class Main {
	public static void main(String[] args) {

		double a=0, b=0, c=0;
		double D;
		D = b * b - 4 * a * c;
		if (D > 0) {
			double x1, x2;
			x1 = (-b - Math.sqrt(D)) / (2 * a);
			x2 = (-b + Math.sqrt(D)) / (2 * a);
			System.out.println("Корни уравнения: x1 = " + x1 + ", x2 = " + x2);
		} else if (D == 0) {
			double x;
			x = -b / (2 * a);
			System.out.println("Уравнение имеет единственный корень: x = " + x);

		}
	}
}
